{
    "name": "parsingcorner/files-symfony2-bundle",
    "license": "propietary",
    "type": "bundle",
    "homepage": "http://www.trovit.com",
    "description": "Symfony2 Bundle to handle files",
    "keywords": ["symfony2","bundle","files"],
    "autoload": {
        "psr-4": {
            "Parsingcorner\\FilesBundle\\": ""
        }
    },
    "authors": [
        {
            "name": "Parsing Corner",
            "email": "parsingcorner@trovit.com"
        }
    ],
    "repositories": [
        {
            "type":"git",
            "url": "https://github.com/viscat/attribute-validation-bundle.git"
        }
    ],
    "require": {
        "php": "5.5.*",
        "symfony/framework-bundle": "2.6.*",
        "symfony/finder": "2.6.*",
        "parsingcorner/attribute-validation-symfony2-bundle": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phpunit/php-invoker": "*",
        "symfony/browser-kit": "2.6.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
